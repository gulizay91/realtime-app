{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "TwilioService": {
      "AccountSID": "${TWILIO_ACCOUNT_SID}",
      "AuthToken": "${TWILIO_AUTH_TOKEN}",
      "PhoneNumber": "${TWILIO_PHONE_NUMBER}",
      "SmsProviderSettings": {
        "Enable": true,
        "ActiveCountryCodes": [ "+90" ]
      }
    }
  }
}
